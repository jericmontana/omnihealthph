{%- if product.handle != blank -%}
  <button title="Add/Remove {{ product.title | escape }} to your wishlist" type="button" aria-label="Add to wishlist" class="button-wishlist" button-wishlist data-product-handle="{{ product.handle }}">
    {%- render 'icon-heart' -%}{{ label }}
  </button>
{%- endif -%}
